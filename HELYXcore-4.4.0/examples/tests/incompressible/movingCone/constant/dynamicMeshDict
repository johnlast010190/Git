FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      dynamicMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

mover
{
    type            motionSolver;
    libs            ("libfvMeshMovers.so" "libfvMotionSolvers.so");
    motionSolver    velocityComponentLaplacian;
    component       x;
    diffusivity     directional (1 200 0);
}

topoChanger
{
    type            meshToMesh;
    libs            ("libfvMeshTopoChangers.so");
    times           (0.0015 0.003);
    timeDelta       1e-6;
}


