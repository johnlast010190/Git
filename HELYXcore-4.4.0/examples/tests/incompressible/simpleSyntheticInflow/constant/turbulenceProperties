{
    LESModel        kEqn;
    delta           cubeRootVol;
    turbulence      on;
    printCoeffs     on;
    kEqnCoeffs
    {
        Prt             1;
    }
    cubeRootVolCoeffs
    {
        deltaCoeff      1;
    }
