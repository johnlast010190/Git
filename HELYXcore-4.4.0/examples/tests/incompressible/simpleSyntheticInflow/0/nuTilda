FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    bottom
    {
        type            symmetry;
    }
    top
    {
        type            symmetry;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 1e-12;
    }
    outlet
    {
        type            zeroGradient;
    }
    dx
    {
        type            symmetry;
    }
    dy
    {
        type            symmetry;
    }
}


