FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict.mesh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     helyxSolve;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1000;

deltaT          1;

writeControl    timeStep;

writeInterval   1000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable true;

libs            ( "" );

functions
{
    VR_mesh
    {
        functionObjectLibs ( "libreport.so" );
        logToFile       true;
        fields          ( aspectRatio determinant nonOrthogonality skewness volumeRatio );
        executeControl  onEnd;
        writeControl    onEnd;
        type            volumeReport;
        outputFileFormat tsv;
    }
}


