FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application helyxHexMesh;

startFrom latestTime;

startTime 0;

stopAt endTime;

endTime 1;

deltaT constant 1.0;

writeControl timeStep;

writeInterval 1;

purgeWrite 0;

writeFormat ascii;

writePrecision 10;

writeCompression uncompressed;

writeEndTime true;

timeFormat general;

timePrecision 6;

graphFormat raw;

runTimeModifiable yes;

libs ( "" );

functions
{
    VR_mesh
    {
        functionObjectLibs ( "libreport.so" );
        logToFile       yes;
        fields          ( aspectRatio determinant nonOrthogonality skewness volumeRatio );
        executeControl  outputTime;
        writeControl    outputTime;
        type            volumeReport;
        outputFileFormat tsv;
    }
}


