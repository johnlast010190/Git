FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

vertices
(
    ( 0.0 0.0 -50.0 )
    ( 200.0 0.0 -50.0 )
    ( 200.0 70.0 -50.0 )
    ( 0.0 70.0 -50.0 )
    ( 0.0 0.0 50.0 )
    ( 200.0 0.0 50.0 )
    ( 200.0 70.0 50.0 )
    ( 0.0 70.0 50.0 )
);

blocks
(
    hex ( 0 1 2 3 4 5 6 7 ) ( 100 35 50 ) simpleGrading ( 1 1 1 )
);

edges ();

patches
(
    wall inlet ( ( 0 4 7 3 ) )
    wall outlet ( ( 1 2 6 5 ) )
    wall ground ( ( 0 1 5 4 ) )
    wall top ( ( 3 7 6 2 ) )
    wall side1 ( ( 0 3 2 1 ) )
    wall side2 ( ( 4 5 6 7 ) )
);

mergePatchPairs ();


