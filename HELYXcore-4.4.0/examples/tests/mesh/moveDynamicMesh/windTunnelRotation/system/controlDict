FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application moveDynamicMesh;

startFrom latestTime;

startTime 0;

stopAt endTime;

endTime 0.5;

deltaT constant 0.01;

writeControl runTime;

writeInterval 0.1;

purgeWrite 0;

writeFormat ascii;

writePrecision 10;

writeCompression uncompressed;

timeFormat general;

timePrecision 6;

graphFormat raw;

runTimeModifiable true;

adjustTimeStep false;

functions
{
    VR
    {
        type                volumeReport;
        functionObjectLibs  ( "libreport.so" );
        logToFile           true;
        fields              ( nonOrthogonality );
        writeControl        onEnd
        executeControl      onEnd;
        outputFileFormat    tsv;
    }
}


