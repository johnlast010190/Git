// Scaled 1/400
// -------------
// convertToMeters 1;
// vertices
// (
//     ( -10.0 -3.75 0 )
//     ( 10.0 -3.75 0 )
//     ( 10.0 3.75 0 )
//     ( -10.0 3.75 0 )
//     ( -10.0 -3.75 2.6 )
//     ( 10.0 -3.75 2.6 )
