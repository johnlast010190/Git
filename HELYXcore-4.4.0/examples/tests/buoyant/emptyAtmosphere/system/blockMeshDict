//     ( -10.0 3.75 2.6 )
// );
// blocks
// (
//     hex ( 0 1 2 3 4 5 6 7 ) ( 100 60 13 ) simpleGrading ( 1 1 1 )
// );

convertToMeters 1;

vertices
(
    ( -3000.0 -1500.0 0 )
    ( 5000.0 -1500.0 0 )
    ( 5000.0 1500.0 0 )
    ( -3000.0 1500.0 0 )
    ( -3000.0 -1500.0 600 )
    ( 5000.0 -1500.0 600 )
    ( 5000.0 1500.0 600 )
    ( -3000.0 1500.0 600 )
);

blocks
(
    hex ( 0 1 2 3 4 5 6 7 ) ( 100 60 13 ) simpleGrading ( 1 1 1 )
);

edges ();

patches
(
    patch outlet ( ( 0 4 7 3 ) )
    patch inlet ( ( 1 2 6 5 ) )
    wall sides ( ( 0 1 5 4 ) ( 3 7 6 2 ) )
    wall ground ( ( 0 3 2 1 ) )
    wall top ( ( 4 5 6 7 ) )
);

mergePatchPairs ();


