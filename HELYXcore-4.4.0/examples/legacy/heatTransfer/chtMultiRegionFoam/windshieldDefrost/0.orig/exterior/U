FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0.0001/exterior";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    atmosphere
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
    symmetry
    {
        type            symmetryPlane;
    }
    exterior_to_ice
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


