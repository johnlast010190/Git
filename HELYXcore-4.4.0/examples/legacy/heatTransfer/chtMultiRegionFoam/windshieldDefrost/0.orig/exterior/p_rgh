FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0.0001/exterior";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    atmosphere
    {
        type            totalPressure;
        p0              uniform 100000;
    }
    symmetry
    {
        type            symmetryPlane;
    }
    exterior_to_ice
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 100000;
    }
}


