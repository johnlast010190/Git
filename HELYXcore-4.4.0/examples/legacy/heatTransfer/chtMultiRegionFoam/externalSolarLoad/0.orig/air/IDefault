FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/air";
    object      IDefault;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 0 -3 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    maxY
    {
        type            greyDiffusiveRadiation;
        value           uniform 0;
    }
    minX
    {
        type            greyDiffusiveRadiation;
        value           uniform 0;
    }
    maxX
    {
        type            greyDiffusiveRadiation;
        value           uniform 0;
    }
    minY
    {
        type            greyDiffusiveRadiation;
        value           uniform 0;
    }
    maxZ
    {
        type            greyDiffusiveRadiation;
        value           uniform 0;
    }
    air_to_floor
    {
        type            greyDiffusiveRadiation;
        value           uniform 0;
    }
    air_to_solid
    {
        type            greyDiffusiveRadiation;
        value           uniform 0;
    }
}


