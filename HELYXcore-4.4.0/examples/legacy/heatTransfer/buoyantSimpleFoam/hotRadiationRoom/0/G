FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      G;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    floor
    {
        type            MarshakRadiation;
        value           uniform 0;
    }

    fixedWalls
    {
        type            MarshakRadiation;
        value           uniform 0;
    }

    ceiling
    {
        type            MarshakRadiation;
        value           uniform 0;
    }

    box
    {
        type            MarshakRadiation;
        value           uniform 0;
    }
}

