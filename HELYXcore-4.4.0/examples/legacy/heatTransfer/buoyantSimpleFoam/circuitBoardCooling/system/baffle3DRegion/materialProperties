FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      materialProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

materialType     solid;
kappaModel       constSolid;
thermodynamics   hConst;
equationOfState  rhoConst;
energy           sensibleEnthalpy;


molWeight       20;
kappaModelCoeffs
{
    kappa           1;
}
thermodynamicsCoeffs
{
    Hf              0;
    Cp              10;
}
