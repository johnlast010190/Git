FoamFile
{
    version     2.0;
    format      binary;
    class       dictionary;
    arch        "LSB;label=32;scalar=64";
    location    "constant";
    object      dynamicMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

mover
{
    type            motionSolver;
    libs            ( "libfvMeshMovers.so" "libfvMotionSolvers.so" );
    motionSolver    motionSolverList;
    solvers
    {
        rotatingRegion
        {
            solver          solidBody;
            cellZone        rotatingRegion;
            referenceFrame  rotatingFrame;
        }
    }
}


