#!/bin/bash
cd ${0%/*} || exit 1    # Run from this directory

# Source tutorial run functions
. $HELYX_PROJECT_DIR/bin/tools/CleanFunctions

(
    cd wingMotion_helyxHexMesh || exit 1
    cleanCase
)

(
    cd wingMotion2D_simpleFoam || exit 1
    cleanCase
    rm -rf 0
)

(
    cd wingMotion2D_pimpleDyMFoam || exit 1
    cleanCase
    rm -rf 0
)

rm wingMotion_helyxHexMesh/layerInfo.vtk
