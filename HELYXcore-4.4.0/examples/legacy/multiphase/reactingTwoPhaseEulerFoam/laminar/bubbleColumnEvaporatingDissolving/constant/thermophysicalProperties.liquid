FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         multicomponentMixture;
    transport       const;
    thermo          eConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

species
(
    air
    water
);

defaultSpecie water;

"(mixture|water)"
{
    specie
    {
        molWeight   18.0153;
    }
    equationOfState
    {
        rho        1000;
    }
    thermodynamics
    {
        Hf          -1.5879e+07;
        Cv          4195;
    }
    transport
    {
        mu          3.645e-4;
        Pr          2.289;
    }
}

air
{
    specie
    {
        molWeight   28.9;
    }
    equationOfState
    {
        rho         1000;
    }
    thermodynamics
    {
        Hf          0;
        Cv          4195;
    }
    transport
    {
        mu          3.645e-4;
        Pr          2.289;
    }
}


