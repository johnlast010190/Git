FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/wallFilmRegion";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         none;

    div(phid,alpha) Gauss upwind;
    div(phi,U)      Gauss upwind;
    div(phi,e)      Gauss upwind;

    div(nHat)       Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear uncorrected;
}

snGradSchemes
{
    default         uncorrected;
}


