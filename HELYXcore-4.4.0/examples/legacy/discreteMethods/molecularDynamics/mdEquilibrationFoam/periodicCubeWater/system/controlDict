FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     mdEquilibrationFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         5e-12;

deltaT          1e-15;

writeControl    runTime;

writeInterval   2e-13;

purgeWrite      0;

writeFormat     ascii;

writePrecision  12;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  no;

