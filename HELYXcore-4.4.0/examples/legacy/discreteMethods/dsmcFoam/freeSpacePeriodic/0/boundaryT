{
    xPeriodic_half0
    {
        type            cyclic;
    }
    yPeriodic_half0
    {
        type            cyclic;
    }
    zPeriodic_half0
    {
        type            cyclic;
    }
    yPeriodic_half1
    {
        type            cyclic;
    }
    zPeriodic_half1
    {
        type            cyclic;
    }
    xPeriodic_half1
    {
        type            cyclic;
    }
}


