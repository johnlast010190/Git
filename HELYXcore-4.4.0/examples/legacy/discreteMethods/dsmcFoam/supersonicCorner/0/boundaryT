FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      boundaryT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    flow
    {
        type            fixedValue;
        value           uniform 300;
    }

    "entrance.*"
    {
        type            symmetryPlane;
    }

    walls
    {
        type            fixedValue;
        value           uniform 1000;
    }
}

