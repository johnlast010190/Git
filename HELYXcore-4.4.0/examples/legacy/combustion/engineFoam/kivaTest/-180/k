FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "-180";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 4;

boundaryField
{
    piston
    {
        type            kqRWallFunction;
        value           uniform 4;
    }
    liner
    {
        type            kqRWallFunction;
        value           uniform 4;
    }
    cylinderHead
    {
        type            kqRWallFunction;
        value           uniform 4;
    }
}


