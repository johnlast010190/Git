FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "-180";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 373;

boundaryField
{
    piston
    {
        type            fixedValue;
        value           uniform 433;
    }

    liner
    {
        type            fixedValue;
        value           uniform 373;
    }

    cylinderHead
    {
        type            fixedValue;
        value           uniform 383;
    }
}


