FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Su;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform 0.135;

boundaryField
{
    "(walls|cylinder)"
    {
        type            zeroGradient;
    }
    "inlet.*"
    {
        type            fixedValue;
        value           uniform 0.135;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.135;
        value           uniform 0.135;
    }
    frontAndBack
    {
        type            empty;
    }
}


