            O2 { initialisation{type fixedValue; value uniform 0.23301;} }
            N2 { initialisation{type fixedValue; value uniform 0.76699;} }
            FSDomega { initialisation{type fixedValue; value uniform 0;} }
            IDefault { initialisation{type fixedValue; value uniform 0;} }
            Ydefault { initialisation{type fixedValue; value uniform 0;} }
            soot { initialisation{type fixedValue; value uniform 0;} }
        }

        boundaryTypeDefaults
        {
            wall { G { type MarshakRadiation; T T; value uniform 0; } }
            inlet { G { type MarshakRadiation; T T; value uniform 0; } }
            outlet { G { type MarshakRadiation; T T; value uniform 0; } }
        }

        boundaryConditions
        {
            "(outlet|sides)"
            {
                alphat { type zeroGradient; }
                CH4 { type inletOutlet; inletValue uniform 0; value uniform 0; }
                O2 { type inletOutlet; inletValue uniform 0.23301; value uniform 0.23301; }
                N2 { type inletOutlet; inletValue uniform 0.76699; value uniform 0.76699; }
                FSDomega { type inletOutlet; inletValue uniform 0; value uniform 0; }
                IDefault { type greyDiffusiveRadiation; value uniform 0; }
                Ydefault { type inletOutlet; inletValue uniform 0; value uniform 0; }
                soot { type calculated; value uniform 0; }
                k { type inletOutlet; inletValue uniform 1e-4; value uniform 1e-4; }
                nut { type zeroGradient; }
                T { type inletOutlet; inletValue uniform 300; value uniform 300; }
                U { type pressureInletOutletVelocity; value uniform (0 0 0); }
                p { type totalPressure; p0 uniform 101325; value uniform 101325; addStaticHead true; }
                ph { type fixedFluxPressure; value uniform 0;}
            }
            base
            {
                alphat { type zeroGradient; }
                CH4 { type zeroGradient; }
                O2 { type zeroGradient; }
                N2 {  type zeroGradient; }
                FSDomega { type zeroGradient; }
                IDefault { type greyDiffusiveRadiation; value uniform 0; }
                Ydefault { type zeroGradient; }
                soot { type calculated; value uniform 0; }
                k { type zeroGradient; }
                nut { type zeroGradient; }
                T { type zeroGradient; }
                U { type noSlip; }
                p { type fixedFluxPressure; value uniform 101325;}
                ph { type fixedFluxPressure; value uniform 0;}
            }
            inlet
            {
                alphat { type zeroGradient; }
                CH4 { type fixedValue; value uniform 1; }
                O2 { type fixedValue; value uniform 0; }
                N2 { type fixedValue; value uniform 0; }
                FSDomega { type fixedValue; value uniform 0; }
                IDefault { type greyDiffusiveRadiation; value uniform 0; }
                Ydefault { type fixedValue; value uniform 0; }
                soot { type calculated; value uniform 0; }
                k { type fixedValue; value uniform 1e-4; }
                nut { type zeroGradient; }
                T { type fixedValue; value uniform 300; }
                U { type fixedValue; value uniform (0 0.05 0);}
