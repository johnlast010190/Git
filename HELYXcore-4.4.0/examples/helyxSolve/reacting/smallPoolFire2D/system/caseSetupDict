                // }
            }

            g
            {
                dimensions      [0 1 -2 0 0 0 0];
                value           (0 -9.8 0);
            }
        }

        boundaryMesh
        {
            base { type patch; }
            outlet { type outlet; }
            sides { type patch; }
            frontAndBack { type empty; }
            inlet { type inlet; }
        }

        fields
        {
            U { initialisation{type fixedValue; value uniform (0 0 0);} }
            T { initialisation{type fixedValue; value uniform 300;} }
            G { initialisation{type fixedValue; value uniform 0;} }
            p { initialisation{type buoyantPressure; value uniform 101325;} }
            k { initialisation{type fixedValue; value uniform 1e-4;} }
