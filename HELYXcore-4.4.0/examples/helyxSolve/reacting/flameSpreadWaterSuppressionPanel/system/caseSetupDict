                    charReaction
                    {
                        type        irreversibleSolidArrheniusReaction;
                        reaction    "wood^4.86 = char + gas";
                        A           7.83e10;
                        Ta          15274.57;
                        Tcrit       400;
                    }
                }
            }
        }

        fieldMaps
        {
            char.solid phase;
            p p;
            Qr Intensity;
            T.solid T;
            wood.solid phase;
