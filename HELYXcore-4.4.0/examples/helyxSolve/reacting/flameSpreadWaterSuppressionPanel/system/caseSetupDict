            p { initialisation { type buoyantPressure; initialValue uniform 0; } }
            U { initialisation { type fixedValue; value uniform (0 0 0); } }
            C3H8 { initialisation { type fixedValue; value uniform 0; } }
            N2 { initialisation { type fixedValue; value uniform 0.76699; } }
            O2 { initialisation { type fixedValue; value uniform 0.23301; } }
            IDefault { initialisation { type fixedValue; value uniform 0.0; } }
        }
        boundaryConditions
        {
            outlet
            {
                alphat { type calculated; value uniform 0; }
                k { type inletOutlet; inletValue uniform 1e-5; value uniform 1e-5; }
                nut { type calculated; value uniform 0; }
                p { type totalPressure; p0 uniform 0; value uniform 0; addStaticHead true; }
                T { type inletOutlet; inletValue uniform 298; value uniform 298; }
                U { type inletOutlet; value uniform (0 0 0); inletValue uniform (0 0 0); }
                N2 { type inletOutlet; inletValue uniform 0.76699; inputValue uniform 0.76699; value uniform 0.76699; }
                C3H8 { type inletOutlet; inletValue uniform 0; value uniform 0; }
                O2 { type inletOutlet; inletValue uniform 0.23301; value uniform 0.23301; }
                H2O { type inletOutlet; inletValue uniform 0; value uniform 0; }
                CO2 { type inletOutlet; inletValue uniform 0; value uniform 0; }
                Ydefault { type inletOutlet; inletValue uniform 0.0; value uniform 0.0; }
                IDefault { type greyDiffusiveRadiation; value uniform 0; }
            }
            inlet
            {
                alphat { type calculated; value uniform 0; }
                k { type fixedValue; value uniform 1e-5; }
                nut { type calculated; value uniform 0; }
                p { type fixedFluxPressure; value uniform 0; }
                T { type fixedValue; value uniform 298.0; }
                U { type flowRateInletVelocity; massFlowRate constant 0.01; value uniform (0 0 0); }
                O2 { type fixedValue; value uniform 0; }
                N2 { type fixedValue; value uniform 0; }
                C3H8 { type totalFlowRateAdvectiveDiffusive; value uniform 1; }
                H2O { type fixedValue; value uniform 0; }
                CO2 { type fixedValue; value uniform 0; }
                Ydefault { type fixedValue; value uniform 0; }
                IDefault { type greyDiffusiveRadiation; value uniform 0; }
            }
            ground
            {
                alphat { type alphatWallFunction; value uniform 0; }
                k { type zeroGradient; }
                nut { type nutUSpaldingWallFunction; value uniform 0; }
                p { type fixedFluxPressure; value uniform 0; }
                T { type zeroGradient; }
                U { type fixedValue; value uniform (0 0 0); }
                C3H8 { type zeroGradient; }
                N2 { type zeroGradient; }
                O2 { type zeroGradient; }
                H2O { type zeroGradient; }
                CO2 { type zeroGradient; }
                Ydefault { type zeroGradient; }
                IDefault { type greyDiffusiveRadiation; value uniform 0; }
            }
            side
            {
                alphat { type calculated; value uniform 0; }
                k { type inletOutlet; inletValue uniform 1e-5; value uniform 1e-5; }
                nut { type calculated; value uniform 0; }
                p { type totalPressure; p0 uniform 0; value uniform 0; addStaticHead true; }
                T { type inletOutlet; inletValue uniform 298; value uniform 298; }
                U { type pressureInletOutletVelocity; value uniform (0 0 0); }
                C3H8 { type inletOutlet; inletValue uniform 0; value uniform 0; }
                N2 { type inletOutlet; inletValue uniform 0.76699; inputValue uniform 0.76699; value uniform 0.76699; }
                O2 { type inletOutlet; inletValue uniform 0.23301; value uniform 0.23301; }
                H2O { type inletOutlet; inletValue uniform 0.0; value uniform 0.0; }
                CO2 { type inletOutlet; inletValue uniform 0.0; value uniform 0.0; }
                Ydefault { type inletOutlet; inletValue uniform 0.0; value uniform 0.0; }
                IDefault { type greyDiffusiveRadiation; value uniform 0; }
            }
            region0_to_pyrolysisRegion_coupledWall
            {
                alphat { type alphatWallFunction; value uniform 0; }
                k { type zeroGradient; }
                nut { type nutUSpaldingWallFunction; value uniform 0; }
                p { type fixedFluxPressure; value uniform 0; }
                T
                {
                    type            filmPyrolysisRadiativeCoupledMixed;
                    pyrolysisRegion pyrolysisRegion;
                    filmRegion      filmRegion;
                    Tnbr            T.solid;
                    kappaMethod     fluidThermo;
                    QrNbr           none;
                    Qr              Qr;
                    filmDeltaDry    0.0;
                    filmDeltaWet    2e-4;
                    value           uniform 298;
                }
                U { type mappedFlowRate; nbrPhi phiGas; value uniform (0 0 0); }
                C3H8 { type totalFlowRateAdvectiveDiffusive; massFluxFraction 1.0; value uniform 0; }
                O2 { type totalFlowRateAdvectiveDiffusive; massFluxFraction 0.0; value uniform 0.23301; }
                N2 { type totalFlowRateAdvectiveDiffusive; massFluxFraction 0.0; value uniform 0.76699; }
                H2O { type totalFlowRateAdvectiveDiffusive; massFluxFraction 0.0; value uniform 0.0; }
                CO2 { type totalFlowRateAdvectiveDiffusive; massFluxFraction 0.0; value uniform 0.0; }
                Ydefault { type totalFlowRateAdvectiveDiffusive; massFluxFraction 0.0; value uniform 0; }
                IDefault { type greyDiffusiveRadiation; value uniform 0; }
            }
        }
    }
}


