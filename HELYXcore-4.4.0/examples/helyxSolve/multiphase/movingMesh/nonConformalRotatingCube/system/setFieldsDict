FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    boxToCell
    {
        box (-1e15 -1e15 -1e15) (1e15 0 1e15);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }

    boxToFace
    {
        box (-1e15 -1e15 -1e15) (1e15 0 1e15);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);


