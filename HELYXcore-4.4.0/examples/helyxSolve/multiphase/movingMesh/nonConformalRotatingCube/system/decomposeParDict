FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 8;

method          hierarchical;

distributed     false;

hierarchicalCoeffs
{
    n               (2 2 2);
    order           xyz;
}

constraints
{
    refinementHistory
    {
        type            refinementHistory;
    }
}


