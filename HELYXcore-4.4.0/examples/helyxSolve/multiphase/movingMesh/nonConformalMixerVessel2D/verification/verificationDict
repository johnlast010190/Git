runs
(
    short
    {
        steps 10;
        checks
        (
            {
                result "postProcessing/VR/0/volumeReport.tsv";
                reference "verification/short/VR/0/volumeReport.tsv";
                variables "Umax" "Umean" "pmax" "pmean" "alpha.watermax" "alpha.watermean" "totalVolume";
            }
        );
    }
    long
    {
        steps 50;
        checks
        (
            {
                result "postProcessing/VR/0/volumeReport.tsv";
                reference "verification/long/VR/0/volumeReport.tsv";
                variables "Umax" "Umean" "pmax" "pmean" "alpha.watermax" "alpha.watermean" "totalVolume";
            }
        );
    }
);

tags asan;

unstable true;
operatingSystem windows;
