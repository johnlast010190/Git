FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

xMin    -0.06;
xMax    0.06;
yMin    -0.01;
yMax    0.015;
zMin    -0.11;
zMax    0.12;
cellsX  60;
cellsY  13;
cellsZ  115;

vertices
(
    ( $xMin  $yMin  $zMin )
    ( $xMax  $yMin  $zMin )
    ( $xMax  $yMax  $zMin )
    ( $xMin  $yMax  $zMin )
    ( $xMin  $yMin  $zMax )
    ( $xMax  $yMin  $zMax )
    ( $xMax  $yMax  $zMax )
    ( $xMin  $yMax  $zMax )
);

blocks
(
    hex (0 1 2 3 4 5 6 7) ($cellsX $cellsY $cellsZ) simpleGrading (1 1 1)
);

edges ();

boundary
(
    xMin
    {
        type patch;
        faces
        (
            (0 4 7 3)
        );
    }
    xMax
    {
        type patch;
        faces
        (
            (1 5 6 2)
        );
    }
    yMin
    {
        type patch;
        faces
        (
            (0 1 2 3)
        );
    }
    yMax
    {
        type patch;
        faces
        (
            (4 5 6 7)
        );
    }
    zMin
    {
        type patch;
        faces
        (
            (1 0 4 5)
        );
    }
    zMax
    {
        type patch;
        faces
        (
            (3 7 6 2)
        );
    }
);


