FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.1;

vertices
(
  ( -1.0 -0.005 -1.0)
  ( 1.0 -0.005 -1.0)
  ( 1.0 0.005 -1.0)
  ( -1.0 0.005 -1.0)
  ( -1.0 -0.005 1.0)
  ( 1.0 -0.005 1.0)
  ( 1.0 0.005 1.0)
  ( -1.0 0.005 1.0)
);

blocks ( hex ( 0 1 2 3 4 5 6 7) ( 50 1 50) simpleGrading ( 1 1 1));

edges ();

patches
(
  wall side1 ( (0 4 7 3) )
  wall side2 ( (1 2 6 5) )
  empty empty1 ( (0 1 5 4) )
  empty empty2 ( (3 7 6 2) )
  wall bottom ( (0 3 2 1) )
  wall top ( (4 5 6 7) )
);

mergePatchPairs ();


