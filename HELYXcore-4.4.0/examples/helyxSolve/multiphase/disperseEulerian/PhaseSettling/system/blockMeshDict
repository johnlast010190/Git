FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

vertices
(
    (-0.5 -0.5 -0.5)
    ( 0.5 -0.5 -0.5)
    ( 0.5  0.5 -0.5)
    (-0.5  0.5 -0.5)

    (-0.5 -0.5  0.5)
    ( 0.5 -0.5  0.5)
    ( 0.5  0.5  0.5)
    (-0.5  0.5  0.5)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (20 20 20) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    sides
    {
        type wall;
        faces
        (
            (1 5 4 0)
            (2 6 5 1)
            (0 4 7 3)
            (3 7 6 2)
        );
    }
    top
    {
        type inlet;
        faces
        (
            (4 5 6 7)
        );
    }
    bottom
    {
        type wall;
        faces
        (
            (0 3 2 1)
        );
    }
);

