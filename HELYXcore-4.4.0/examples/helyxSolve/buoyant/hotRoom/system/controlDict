FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     helyxSolve;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         2000;

deltaT          constant 2;

writeControl    timeStep;

writeInterval   100;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable yes;

libs            1 ( "" );

adjustTimeStep  false;

maxCo           1;

maxDeltaT       1;

functions
{
    VP
    {
        functionObjectLibs ( "libsampling.so" );
        enabled         true;
        probeLocations  ( ( 5 1 5 ) ( 5 4 5 ) );
        fields          ( U );
        executeControl  onEnd;
        type            probes;
        regions         ( region0 );
        writeControl    onEnd;
        outputFileFormat tsv;
    }
}


