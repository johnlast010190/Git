FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      geometryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaces
{
    fire
    {
        type searchableBox;
        min ( -0.75 -0.75 -3.0 );
        max ( 0.75 0.75 -2.5 );
    }
}


