global
{
    modificationSwitches
    {
        resetInternalFields true;
        resetBoundaryFields true;
        resetSystemDicts true;
        resetConstDicts true;
        resetBoundaryMesh true;
        deleteUnusedFields true;
        reuseExistingDicts false;
        strictPatchNameChecking false;
    }
    system
    {
        controlDict
        {
            application helyxSolve;
            startFrom latestTime;
            startTime 0;
            stopAt endTime;
            endTime 10;
            deltaT constant 0.2;
            writeControl timeStep;
            writeInterval 10;
            purgeWrite 0;
            writeFormat ascii;
            writePrecision 10;
            writeCompression uncompressed;
            timeFormat general;
            timePrecision 6;
            graphFormat raw;
            runTimeModifiable yes;
            adjustTimeStep false;
            maxCo 0;
            maxAlphaCo 0;
            maxDeltaT 0;
        }
    }
    functions
    {
        VR
        {
            type             volumeReport;
            fields           ( T U k omega p smoke );
            executeControl   onEnd;
            writeControl     onEnd;
            outputFileFormat tsv;
        }
        visibility
        {
            type            visibilityIndex;
            rho             rho;
            C               3;
            Lmax            30;
            kappam          8700;
            executeControl  timeStep;
            writeControl    writeTime;
            libs            ( "libfieldFunctionObjects.so" );
            regions         1 ( region0 );
        }
        SR-gate
        {
            type surfaceReport;
            regions ( region0 );
            fields ( T U p smoke );
            weighting flux;
            fluxName phi;
            homogeneity false;
            surfaces
            (
                {
                    type patch;
                    name gate;
                }
            );
            constraints
            (
                {
                    type boundBox;
                    min ( -1000.0 -1000.0 -1000.0 );
                    max ( 1000.0 1000.0 1000.0 );
                    keepInside true;
                }
            );
            writeControl timeStep;
            writeInterval 1;
            executeControl timeStep;
            executeInterval 1;
            functionObjectLibs ( "libreport.so" );
            outputFileFormat tsv;
        }
        SR-open1
        {
            type surfaceReport;
            regions ( region0 );
            fields ( T U p smoke );
            weighting flux;
            fluxName phi;
            homogeneity false;
            surfaces
            (
                {
                    type patch;
                    name open1;
                }
            );
            constraints
            (
                {
                    type boundBox;
                    min ( -1000.0 -1000.0 -1000.0 );
                    max ( 1000.0 1000.0 1000.0 );
                    keepInside true;
                }
            );
            writeControl timeStep;
            writeInterval 1;
            executeControl timeStep;
            executeInterval 1;
            functionObjectLibs ( "libreport.so" );
            outputFileFormat tsv;
        }
        SR-open2
        {
            type surfaceReport;
            regions ( region0 );
            fields ( T U p smoke );
            weighting flux;
            fluxName phi;
            homogeneity false;
