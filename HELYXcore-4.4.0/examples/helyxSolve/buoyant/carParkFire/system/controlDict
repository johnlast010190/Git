FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     helyxSolve;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         10;

deltaT          constant 0.2;

writeControl    timeStep;

writeInterval   10;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable yes;

libs            1 ( "" );

adjustTimeStep  false;

maxCo           0;

maxDeltaT       0;

maxAlphaCo      0;

functions
{
    VR
    {
        functionObjectLibs ( "libreport.so" );
        logToFile       yes;
        fields          ( T U k omega p smoke );
        executeControl  onEnd;
        type            volumeReport;
        writeControl    onEnd;
        outputFileFormat tsv;
    }
    visibility
    {
        type            visibilityIndex;
        rho             rho;
        C               3;
        Lmax            30;
        kappam          8700;
        executeControl  timeStep;
        writeControl    writeTime;
        libs            ( "libfieldFunctionObjects.so" );
        regions         1 ( region0 );
    }
    SR-gate
    {
        functionObjectLibs ( "libreport.so" );
        logToFile       yes;
        weighting       flux;
        fluxName        phi;
        fields          ( T U p smoke );
        surfaces        ( { type patch ; name gate ; } );
        constraints     ( { type boundBox ; min ( -1000 -1000 -1000 ) ; max ( 1000 1000 1000 ) ; keepInside true ; } );
        executeControl  timeStep;
        type            surfaceReport;
        regions         ( region0 );
        homogeneity     false;
        writeControl    timeStep;
        writeInterval   1;
        executeInterval 1;
        outputFileFormat tsv;
    }
    SR-open1
    {
        functionObjectLibs ( "libreport.so" );
        logToFile       yes;
        weighting       flux;
        fluxName        phi;
        fields          ( T U p smoke );
        surfaces        ( { type patch ; name open1 ; } );
        constraints     ( { type boundBox ; min ( -1000 -1000 -1000 ) ; max ( 1000 1000 1000 ) ; keepInside true ; } );
        executeControl  timeStep;
        type            surfaceReport;
        regions         ( region0 );
        homogeneity     false;
        writeControl    timeStep;
        writeInterval   1;
        executeInterval 1;
        outputFileFormat tsv;
    }
    SR-open2
    {
        functionObjectLibs ( "libreport.so" );
        logToFile       yes;
        weighting       flux;
        fluxName        phi;
        fields          ( T U p smoke );
        surfaces        ( { type patch ; name open2 ; } );
        constraints     ( { type boundBox ; min ( -1000 -1000 -1000 ) ; max ( 1000 1000 1000 ) ; keepInside true ; } );
        executeControl  timeStep;
        type            surfaceReport;
        regions         ( region0 );
        homogeneity     false;
        writeControl    timeStep;
        writeInterval   1;
        executeInterval 1;
        outputFileFormat tsv;
    }
}


