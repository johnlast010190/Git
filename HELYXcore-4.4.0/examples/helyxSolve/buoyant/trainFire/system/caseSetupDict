global
{
    modificationSwitches
    {
        resetInternalFields true;
        resetBoundaryFields true;
        resetSystemDicts true;
        resetConstDicts true;
        resetBoundaryMesh true;
        deleteUnusedFields true;
        reuseExistingDicts false;
        strictPatchNameChecking false;
    }
    system
    {
        controlDict
        {
            application helyxSolve;
            startFrom startTime;
            startTime 0;
            stopAt endTime;
            endTime 100.0;
            deltaT constant 0.5;
            writeControl runTime;
            writeInterval 10.0;
            writeFormat binary;
            writePrecision 10;
            adjustTimeStep false;
            maxCo 0;
            maxDeltaT 0;
            maxAlphaCo 0;
            minDeltaT 0;
        }
    }
    functions
    {
        FE
        {
            type fractionalEffectiveDose;
            field fed;
            regionType all;
            activityLevel lightWork;
            periodicReset false;
            executeControl timeStep;
            executeInterval 1;
            writeControl outputTimeAndEnd;
            writeInterval 1;
            functionObjectLibs ("libfieldFunctionObjects.so");
        }
        VR
        {
            type volumeReport;
            regions ( region0 );
            fields ( T U k omega p );
            writeControl timeStep;
            writeInterval 1;
            executeControl timeStep;
            executeInterval 1;
            functionObjectLibs ("libreport.so");
        }
        SR_out
        {
            type surfaceReport;
            regions ( region0 );
            fields ( CO CO2 N2 O2 T U p soot );
            weighting flux;
            fluxName phi;
            homogeneity false;
            backFlowReport false;
            directionAware true;
            surfaces
            (
                {
                    type patch;
                    name fan;
                }
            );
            constraints
            (
                {
                    type boundBox;
                    min ( -1000.0 -1000.0 -1000.0 );
                    max ( 1000.0 1000.0 1000.0 );
                    keepInside true;
                }
            );
            writeControl timeStep;
            writeInterval 1;
            executeControl timeStep;
            executeInterval 1;
            functionObjectLibs ("libreport.so" );
        }
    }
}

