FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      caseSetupDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

global
{
    system
    {
        controlDict
        {
            application helyxSolve;
            startFrom latestTime;
            startTime 0;
            stopAt endTime;
            endTime 3000.0;
            deltaT constant 1.0;
            writeControl timeStep;
            writeInterval 500.0;
            adjustTimeStep false;
        }
    }
    functions
    {
        VR
        {
            functionObjectLibs ( "libreport.so" );
            logToFile       yes;
            fields          ( T U p w );
            executeControl  timeStep;
            type            volumeReport;
            writeControl    timeStep;
            writeInterval   10;
            regions         1 ( region0 );
        }
    }
}

regions
{
    region0
    {
        state (steady ras radiation humidity energy);
        boundaryMesh
        {
            Inlet { type inlet; }
            Outlet { type outlet; }
            Floor { type wall; }
            Top { type wall; }
            defaultFaces { type symmetry; }
        }
        turbulenceModel kEpsilon;
        materials (air);
        materialProperties
        {
            air
            {
                materialName    air;
                materialType    fluid;

                energy          sensibleInternalEnergy;

                equationOfState rhoConst;
                equationOfStateCoeffs
                {
                    rho 1.225;
                }

                molWeight 28.9;

                buoyancyModel   Boussinesq;
                buoyancyModelCoeffs
                {
                    rho0        1.225;
                    T0          303.15;
                    beta        0.00333;
                }

                thermodynamics  hConst;
                thermodynamicsCoeffs
                {
                    Hf          0.0;
                    Cp          1003.5;
                }

                muModel         scalarFunction1;
                muModelCoeffs
                {
                    mu          1.77625E-5;
                }

                kappaModel      kappaPrandtl;
                kappaModelCoeffs
                {
                    Pr          0.7;
                }

                referenceFields
                {
                    p [1 -1 -2 0 0 0 0] 101325.0;
                }

                wconcentrationTransport
                {
                    D               [ 0 2 -1 0 0 0 0 ] 2.42e-05;
                    Cp              [ 0 2 -2 -1 0 0 0 ] 1930;
                    Sct             0.7;
                }
            }
        }
        constant
        {
            g
            {
                dimensions [0  1 -2  0 0 0 0];
                value ( 0.0 0.0 -9.81 );
            }
            radiationProperties
            {
                radiation       on;

                participating   no;

                radiationModel  fvDOM;

                fvDOMCoeffs
                {
                    nPhi            2;
                    nTheta          4;
                    convergence     0.001;
                    initialConvergence 0.001;
                    maxIter         10;
                    initialMaxIter  50;
                }

                solverFreq      10;

                solarLoadMode DOM;
                domSolarCoeffs
                {
                    sources
                    (
                        solar
                        {
                            radiantInputType userDefined;
                            intensity constant 800.0;
                            direction constant ( 1 0 -1 );
                            solve true;
                            nSolutions 500;
                        }
                    );
                    sourcePatches ( Inlet Top );
                }
            }
            boundaryRadiationProperties
            {
                Inlet
                {
                    mode    lookup;
                    emissivity  0.0;
                    transmissivity 1.0;
                    transmissivitySolar 0.8;
                    Ta constant 303.15;
                }
                Outlet
                {
                    mode lookup;
                    emissivity 0.0;
                    transmissivity 1.0;
                    transmissivitySolar 0.9;
                    Ta constant 303.15;
                }
                Floor
                {
                    mode lookup;
                    emissivity 0.0;
                    transmissivity 1.0;
                    Ta constant 303.15;
                }
                Top
                {
                    mode lookup;
                    emissivity 0.0;
                    transmissivity 1.0;
                    Ta constant 303.15;
                }
                defaultFaces
                {
                    mode lookup;
                    emissivity 1.0;
                    transmissivity 0.0;
                    Ta constant 300.0;
                }
            }
        }
        system
        {
            fvOptions
            {
                tree_source
                {
                    type vegetationSource;
                    active true;
                    hookOp solve;  // solve leaf heat balance
                    vegetationSourceCoeffs
                    {
                        selectionMode cellZone;
                        cellZone tree;
                        fields ( U k epsilon T w );
                        origin ( 0 0 0.5 ); // at the base of the cellZone
                        direction ( 0 0 1 ); // tree vertical direction
                        Cd 0.2;
                        LADmax 10;
                        LADProfile ( 1 1 );
                    }
                }
            }
        }
        fields
        {
            U { initialisation { type potentialFlow; } }
            p { initialisation { type buoyantPressure; initialValue uniform 0; } }
            k { initialisation { type turbulentIL; I 0.05; L 0.01; Uref 1.0; } }
            epsilon { initialisation { type turbulentIL; I 0.05; L 0.01; Uref 1.0; } }
            T { initialisation { type fixedValue; value uniform 303.0; } }
            w
            {
                initialisation
                {
                    type            relativeHumidity;
                    rho             buoyantRho;
                    relativeHumidity 0.6;
                    Pabs            101325;
                }
            }
        }
        boundaryConditions
        {
            Inlet
            {
                U
                {
                    type atmBoundaryLayerInletVelocity;
                    value uniform ( 1.0 0.0 0.0 );
                    Uref 1;
                    Zref 1;
                    zDir ( 0 0 1 );
                    flowDir ( 1 0 0 );
                    z0 uniform 0.0217;
                    zGround uniform 0.0;
                }
                T
                {
                    type fixedValue;
                    value uniform 303.15;
                }
                k
                {
                    type atmBoundaryLayerInletK;
                    value uniform 1.3;
                    Uref 1;
                    Zref 1;
                    zDir ( 0 0 1 );
                    flowDir ( 1 0 0 );
                    z0 uniform 0.0217;
                    zGround uniform 0.0;
                }
                epsilon
                {
                    type atmBoundaryLayerInletEpsilon;
                    value uniform 0.01;
                    Uref 1;
                    Zref 1;
                    zDir ( 0 0 1 );
                    flowDir ( 1 0 0 );
                    z0 uniform 0.0217;
                    zGround uniform 0.0;
                }
                w
                {
                    type relativeHumidityInletOutlet;
                    value uniform 0.0;
                    relativeHumidity uniform 0.6;
                    rho buoyantRho;
                }
            }
            Outlet
            {
                U
                {
                    type inletOutlet;
                    value uniform ( 0.0 0.0 0.0 );
                    inletValue uniform ( 0.0 0.0 0.0 );
                }
                p
                {
                    type            totalPressure;
                    addStaticHead   true;
                    p0              uniform 0;
                    value           uniform 0;
                }
                T
                {
                    type inletOutlet;
                    value uniform 300.0;
                    inletValue uniform 303.15;
                }
                k
                {
                    type turbulentIntensityKineticEnergyInlet;
                    intensity 0.05;
                    value uniform 0.1;
                }
                epsilon
                {
                    type turbulentMixingLengthDissipationRateInlet;
                    mixingLength 0.01;
                    value uniform 0.01;
                }
                w
                {
                    type relativeHumidityInletOutlet;
                    relativeHumidity uniform 0.6;
                    value uniform 0;
                    rho buoyantRho;
                }
            }
            Top
            {
                U { type slip; }
                k { type zeroGradient; }
                epsilon { type zeroGradient; }
                nut { type zeroGradient; }
                w { type zeroGradient; }
            }
        }
    }
}


