FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      caseSetupDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

global
{
    system
    {
        controlDict
        {
            startFrom latestTime;
            startTime 0;
            stopAt endTime;
            endTime 14400.0;
            deltaT constant 5.0;
            writeControl runTime;
            writeInterval 1800.0;
            adjustTimeStep false;
        }
    }
    functions
    {
        SR
        {
            type surfaceReport;
            regions ( region0 );
            fields ( T );
            weighting area;
            fluxName phi;
            homogeneity false;
            surfaces
            (
                {
                    type patch;
                    name building;
                }
            );
            constraints
            (
                {
                    type boundBox;
                    min ( -1000.0 -1000.0 -1000.0 );
                    max ( 1000.0 1000.0 1000.0 );
                    keepInside true;
                }
            );
            writeControl timeStep;
            writeInterval 1;
            executeControl timeStep;
            executeInterval 1;
            functionObjectLibs ("libreport.so" );
        }
        VR
        {
            type volumeReport;
            regions ( region0 );
            fields ( U );
            writeControl timeStep;
            writeInterval 1;
            executeControl timeStep;
            executeInterval 1;
            functionObjectLibs ("libreport.so" );
        }
    }
}

