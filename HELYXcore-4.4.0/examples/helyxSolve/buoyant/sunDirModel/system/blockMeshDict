{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

vertices
(
    ( -5.0 -5.0 0.0 )
    ( 5.0 -5.0 0.0 )
    ( 5.0 5.0 0.0 )
    ( -5.0 5.0 0.0 )
    ( -5.0 -5.0 10.0 )
    ( 5.0 -5.0 10.0 )
    ( 5.0 5.0 10.0 )
    ( -5.0 5.0 10.0 )
);

blocks
(
    hex ( 0 1 2 3 4 5 6 7 ) ( 20 20 20 ) simpleGrading ( 1 1 1 )
);

edges ();

patches
(
    wall s1 ( ( 0 4 7 3 ) )
    wall s2 ( ( 1 2 6 5 ) )
    wall s3 ( ( 0 1 5 4 ) )
    wall s4 ( ( 3 7 6 2 ) )
    wall ground ( ( 0 3 2 1 ) )
    wall sky ( ( 4 5 6 7 ) )
);

mergePatchPairs ();


