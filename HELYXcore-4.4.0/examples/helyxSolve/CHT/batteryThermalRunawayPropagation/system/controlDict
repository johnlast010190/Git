FoamFile
{
    version     2.0;
    format      binary;
    class       dictionary;
    arch        "LSB;label=32;scalar=64";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         200;

deltaT          constant 1;

writeControl    timeStep;

writeInterval   1;

purgeWrite      0;

writeFormat     binary;

writePrecision  10;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable yes;

application     helyxSolve;

adjustTimeStep  false;

maxCo           0;

maxDeltaT       0;

libs            3 ( "" "libHelyxCoupled.so" "" );

writeEndTime    true;

maxAlphaCo      0;

minDeltaT       0;

meshChangers    false;

functions
{
    TC-battery1
    {
        type            expressionField;
        regions         ( battery1 );
        executeControl  writeTime;
        executeInterval 1;
        writeControl    writeTime;
        writeInterval   1;
        fieldName       TC;
        expression      "T-273.15";
        fieldType       scalar;
        autowrite       true;
        functionObjectLibs ( "libswakFunctionObjects.so" );
    }
    VR-battery1
    {
        functionObjectLibs ( "libreport.so" );
        logToFile       yes;
        fields          ( T TC );
        executeControl  timeStep;
        type            volumeReport;
        regions         ( battery1 );
        writeControl    timeStep;
        writeInterval   1;
        executeInterval 1;
    }
    TC-battery2
