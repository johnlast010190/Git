FoamFile
{
    version     2.0;
    format      binary;
    class       dictionary;
    arch        "LSB;label=32;scalar=64";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     helyxSolve;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         100;

deltaT          constant 1;

writeControl    timeStep;

writeInterval   100;

purgeWrite      0;

writeFormat     binary;

writePrecision  10;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable yes;

libs            1 ( "" );

functions
{
    VR1
    {
        functionObjectLibs ( "libreport.so" );
        logToFile       yes;
        fields          ( U k epsilon p );
        executeControl  timeStep;
        type            volumeReport;
        instance        d1;
        writeControl    timeStep;
        regions         1 ( d1 );
    }
    probes1
    {
        functionObjectLibs ( "libsampling.so" );
        enabled         true;
        probeLocations  ( ( 15 0 2 ) ( 15 20 2 ) );
        fields          ( U p );
        executeControl  timeStep;
        type            probes;
        name            probes;
        instance        d1;
        writeControl    writeTime;
        writeInterval   1;
        libs            ( "libsampling.so" );
        regions         1 ( d1 );
    }
