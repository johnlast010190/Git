FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     helyxSolve;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         0.2;

deltaT          constant 0.001;

writeControl    runTime;

writeInterval   0.05;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable yes;

libs            1 ( "" );

adjustTimeStep  no;

maxCo           1;

maxDeltaT       1;

functions
{
    FP
    {
        functionObjectLibs ( "libfieldFunctionObjects.so" );
        operations      ( { operation ptot ; fieldName ptot ; write true ; nearCellValue false ; U U ; p p ; } );
        executeControl  timeStep;
        writeControl    outputTimeAndEnd;
        type            fieldProcess;
        regions         ( region0 );
        executeInterval 1;
        writeInterval   1;
    }
    FR
    {
        functionObjectLibs ( "libforces.so" );
        directForceDensity false;
        p               p;
        U               U;
        rhoName         rho;
        rhoInf          1;
        pRef            0;
        CofR            ( 0 0 0 );
        patches         ( Co2_BladeLE Co2_BladePS Co2_BladeSS Co2_BladeTE );
        executeControl  timeStep;
        type            forces;
        regions         ( region0 );
        writeControl    timeStep;
        writeInterval   1;
        executeInterval 1;
    }
}


