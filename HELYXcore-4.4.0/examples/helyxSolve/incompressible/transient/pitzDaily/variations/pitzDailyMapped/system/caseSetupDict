                sampleMode      nearestCell;
                samplePatch     none;
            }
            outlet
            {
                type patch;
            }
            "(upperWall|lowerWall)"
            {
                type wall;
            }
            frontAndBack
            {
                type empty;
            }
        }
        turbulenceModel dynamicKEqn;
        materials (air);
        constant
        {
            turbulenceProperties
            {
                simulationType  LES;
                LES
                {
                    LESModel        kEqn;
                    turbulence      on;
                    printCoeffs     on;
                    delta           cubeRootVol;
                    cubeRootVolCoeffs
                    {
                        deltaCoeff      1;
