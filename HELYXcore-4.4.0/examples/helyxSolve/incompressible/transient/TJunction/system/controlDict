FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     helyxSolve;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1.5;

deltaT          0.001;

writeControl    adjustableRunTime;

writeInterval   0.1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable yes;

libs            1 ( "" );

adjustTimeStep  yes;

maxCo           5;

maxDeltaT       1;

functions
{
    probes
    {
        functionObjectLibs ( "libsampling.so" );
        enabled         true;
        probeLocations  ( ( 1e-06 0 0.01 ) ( 0.21 -0.20999 0.01 ) ( 0.21 0.20999 0.01 ) ( 0.21 0 0.01 ) );
        fields          ( p U );
        executeControl  timeStep;
        libs            ( "libsampling.so" );
        type            probes;
        name            probes;
        writeControl    writeTime;
        writeInterval   1;
    }
}


