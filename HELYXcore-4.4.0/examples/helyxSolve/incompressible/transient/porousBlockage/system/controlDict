FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     helyxSolve;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         10;

deltaT          constant 0.05;

writeControl    runTime;

writeInterval   2;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable true;

libs            1 ( "" );

adjustTimeStep  false;

maxCo           0;

maxDeltaT       0;

maxAlphaCo      0;

functions
{
    VR0
    {
        functionObjectLibs ( "libreport.so" );
        logToFile       yes;
        fields          ( U );
        executeControl  onEnd;
        type            volumeReport;
        sets            
        (
            zoneToCell
            {
                name            porousBlockage;
            }

        )
;
        regions         ( region0 );
        writeControl    onEnd;
        outputFileFormat tsv;
    }
    VR1
    {
        functionObjectLibs ( "libreport.so" );
        logToFile       yes;
        fields          ( U p );
        executeControl  onEnd;
        type            volumeReport;
        regions         ( region0 );
        writeControl    onEnd;
        outputFileFormat tsv;
    }
}


