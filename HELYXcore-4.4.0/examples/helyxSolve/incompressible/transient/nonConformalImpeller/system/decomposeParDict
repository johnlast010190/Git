FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 4;

// Keep owner and neighbour on same processor for faces in cyclic patches
preservePatches (cyclicOut1 cyclicOut2);

method          hierarchical;

distributed     false;

hierarchicalCoeffs
{
    n               (2 2 1);
    order           xyz;
}


