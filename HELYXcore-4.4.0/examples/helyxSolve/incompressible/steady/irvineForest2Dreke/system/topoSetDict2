actions
(
    // forest cellZone
    {
        name    forestCells;
        type    cellSet;
        action  new;
        source  boxToCell;
        sourceInfo
        {
            box (0 -100 -100) (1000 100 7.5);
        }
    }
    {
        name    forest;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        sourceInfo
        {
            set forestCells;
        }
    }
);


