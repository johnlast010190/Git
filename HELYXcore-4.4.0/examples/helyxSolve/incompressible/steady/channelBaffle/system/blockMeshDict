FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

vertices
(
    ( -2.0 -0.1 -0.01 )
    ( 2.0 -0.1 -0.01 )
    ( 2.0 0.1 -0.01 )
    ( -2.0 0.1 -0.01 )
    ( -2.0 -0.1 0.01 )
    ( 2.0 -0.1 0.01 )
    ( 2.0 0.1 0.01 )
    ( -2.0 0.1 0.01 )
);

blocks
(
    hex ( 0 1 2 3 4 5 6 7 ) ( 100 30 1 ) simpleGrading ( 1 1 1 )
);

edges ();

patches
(
    wall inlet ( ( 0 4 7 3 ) )
    wall outlet ( ( 1 2 6 5 ) )
    wall bottom ( ( 0 1 5 4 ) )
    wall top ( ( 3 7 6 2 ) )
    wall back ( ( 0 3 2 1 ) )
    wall front ( ( 4 5 6 7 ) )
);

mergePatchPairs ();


