FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     helyxSolve;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         3000;

deltaT          constant 1;

writeControl    timeStep;

writeInterval   500;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable true;

libs            1 ( "" );

adjustTimeStep  false;

maxCo           0;

maxAlphaCo      0;

maxDeltaT       0;

functions
{
    VR
    {
        functionObjectLibs ( "libreport.so" );
        logToFile       yes;
        fields          ( U epsilon k );
        executeControl  onEnd;
        type            volumeReport;
        regions         ( region0 );
        writeControl    onEnd;
        outputFileFormat tsv;
    }
}


