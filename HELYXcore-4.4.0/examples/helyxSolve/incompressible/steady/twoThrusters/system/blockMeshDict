vertices
(
    (-5.0 -1.5 -1.5)
    (5.0 -1.5 -1.5)
    (5.0 1.5 -1.5)
    (-5.0 1.5 -1.5)

    (-5.0 -1.5 1.5)
    (5.0 -1.5 1.5)
    (5.0 1.5 1.5)
    (-5.0 1.5 1.5)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (160 48 48) simpleGrading (1 1 1)
);

edges ();

