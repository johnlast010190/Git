#!/usr/bin/gnuplot -persist
# set terminal postscript eps enhanced color font "Helvetica,24"
set terminal png size 1200,600
reset
set grid

####################################################################
set output "LiftvsIterationAoASweep.png"

set xrange [0:500]
set xtics 100
set xlabel "Iterations"
set yrange [0:1.8]
set ytics 0.2
set ylabel "Lift"
set key outside right

