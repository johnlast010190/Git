FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

room
{
    type            wallVelocity;
    value           uniform ( 0 0 0 );
}

fan1
{
    type            inletOutlet;
    value           uniform ( 0 0 0 );
    inletValue      uniform ( 0 0 0 );
}

fan2
{
    type            inletOutlet;
    value           uniform ( 0 0 0 );
    inletValue      uniform ( 0 0 0 );
}

opening
{
    type            pressureVelocity;
    value           uniform ( 0 0 0 );
}


