FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      modifyGIBBoundaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

boundary
(
    ib1
    {
        type            indirectWall;
        faceZone        wallZone;
        indirectPolyPatchType master;
    }
    ib2
    {
        type            indirectWall;
        faceZone        wallZone;
        indirectPolyPatchType slave;
    }
);

initialization
{
    initializationType GIBChanger;
    libs               ("libfvMeshGIBChangers.so");
    type               solidBody;
    triSurfaceName     mixer.stl;
    region0Patch       (bottom);
}


