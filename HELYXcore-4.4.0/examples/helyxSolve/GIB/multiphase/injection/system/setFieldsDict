FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
    volScalarFieldValue p 0
);

regions
(
    boxToCell
    {
        box (-10 -10 -10) (10 0.02 10);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
    boxToCell
    {
        box (0.03 0.2295 -1) (0.04 0.23025 1);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);


