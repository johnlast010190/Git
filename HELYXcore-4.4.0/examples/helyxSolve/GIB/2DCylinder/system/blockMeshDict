FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

vertices
(
    (-10   -5    0)
    ( 10   -5    0)
    ( 10    5    0)
    (-10    5    0)

    (-10   -5    0.01)
    ( 10   -5    0.01)
    ( 10    5    0.01)
    (-10    5    0.01)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (200 100 1) simpleGrading (1 1 1)
);

edges ();

boundary
(
    Inlet
    {
        type inlet;
        faces
        (
            (0 3 7 4)
        );
    }
    Outlet
    {
        type outlet;
        faces
        (
            (1 2 6 5)
        );
    }
    Walls
    {
        type wall;
        faces
        (
            (3 2 6 7)
            (0 1 5 4)
        );
    }
    frontAndBack
    {
        type empty;
        faces
        (
            (0 1 2 3)
            (4 5 6 7)
        );
    }

);

mergePatchPairs ();


