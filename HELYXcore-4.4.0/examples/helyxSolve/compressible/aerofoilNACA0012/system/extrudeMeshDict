FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      extrudeMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

constructFrom patch;
sourceCase    ".";

sourcePatches (back);
exposedPatchName front;

extrudeModel  linearDirection;
direction     (0 -1 0);
thickness     0.1;

flipNormals false;
mergeFaces false;


