FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      caseSetupDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

global
{
    modificationSwitches
    {
        resetInternalFields yes;
        resetBoundaryFields yes;
        resetSystemDicts yes;
        resetConstDicts yes;
        resetBoundaryMesh yes;
        deleteUnusedFields no;
    }

    regionGroups    1 ( fluid 1 ( region0 ) );

    system
    {
        controlDict
        {
            application     helyxSolve;
            startFrom       latestTime;
            stopAt          endTime;
            endTime         0.0004;
            deltaT          5e-07;
            writeControl    runTime;
            writeInterval   0.0001;
            purgeWrite      0;
        }
    }

    functions
    {
        Ma
        {
            libs            ( "libfieldFunctionObjects.so" );
            type            MachNo;
            executeControl  writeTime;
            writeControl    writeTime;
        }
        VR
        {
            functionObjectLibs ( "libreport.so" );
            logToFile       yes;
            fields          ( U p T );
            executeControl  onEnd;
            type            volumeReport;
            writeControl    onEnd;
            outputFileFormat tsv;
        }
    }
}

regions
{
    region0
    {
        boundaryMesh
        {
            inlet
            {
                type            inlet;
            }
            outlet
            {
                type            outlet;
            }
            bottomWall
            {
                type            wall;
            }
            topWall
            {
                type            wall;
            }
            prismWall
            {
                type            wall;
            }
            defaultFaces
            {
                type            empty;
            }
        }
        state           ( transient ras energy transonic );
        turbulenceModel kEpsilon;
        materials       ( normalisedGas );
        materialProperties
        {
            normalisedGas
            {
                materialType    fluid;
                equationOfState perfectGas;
                energy          sensibleInternalEnergy;
                thermodynamics  hConst;
                thermodynamicsCoeffs
                {
                    Cp              1005;
                    Hf              0;
                }

                muModel         scalarFunction1;
                muModelCoeffs
                {
                    mu 1.8e-05;
                }
                kappaModel      kappaPrandtl;
                kappaModelCoeffs
                {
                    Pr 0.7;
                }

                molWeight       28.9;
            }
        }
        system
        {
            fvSolution
            {
                PIMPLE
                {
                    nOuterCorrectors     2;
                    nCorrectors          1;
                    nNonOrthogonalCorrectors 0;
                }
            }
        }
        fields
        {
            U
            {
                initialisation
                {
                    type            fixedValue;
                    value           uniform ( 650 0 0 );
                }
            }
            p
            {
                initialisation
                {
                    type            fixedValue;
                    value           uniform 1e05;
                }
            }
            T
            {
                initialisation
                {
                    type            fixedValue;
                    value           uniform 300;
                }
            }
        }
        boundaryConditions
        {
            inlet
            {
                U
                {
                    type  fixedValue;
                    value uniform (650 0 0);
                }
                p
                {
                    type            fixedValue;
                    value           uniform 100000;
                }
                T
                {
                    type            fixedValue;
                    value           uniform 300;
                }
            }
            outlet
            {
                p
                {
                    type            waveTransmissive;
                    field           p;
                    psi             psi;
                    gamma           1.3;
                    fieldInf        100000;
                    lInf            1;
                    value           uniform 100000;
                }
                U
                {
                    type            inletOutlet;
                    inletValue      uniform (0 0 0);
                    value           uniform (0 0 0);
                }
                T
                {
                    type            inletOutlet;
                    inletValue      uniform 300;
                    value           uniform 300;
                }
            }
            bottomWall
            {
                U
                {
                    type            supersonicFreestream;
                    pInf            100000;
                    Tinf            300;
                    UInf            (650 0 0);
                    gamma           1.28418;
                    value           uniform (650 0 0);
                }
                T
                {
                    type            inletOutlet;
                    inletValue      uniform 300;
                    value           uniform 300;
                }
            }
            topWall
            {
                U
                {
                    type            supersonicFreestream;
                    pInf            100000;
                    Tinf            300;
                    UInf            (650 0 0);
                    gamma           1.28418;
                    value           uniform (650 0 0);
                }
                T
                {
                    type            inletOutlet;
                    inletValue      uniform 300;
                    value           uniform 300;
                }
            }
            prismWall
            {
                U
                {
                    type noSlip;
                }
                T
                {
                    type zeroGradient;
                }
            }
        }
    }
}


