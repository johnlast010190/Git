FoamFile
{
    version 2.0;
    format ascii;
    class dictionary;
    location system;
    object caseSetupDict;
}

global
{
    regionGroups    ( fluid ( region0 ) );        

    functions
    {
        vr1
        {
            type volumeReport;
            regions ( region0 );
            fields ( rho U p );
            functionObjectLibs ("libreport.so" );
        }
        probes
        {
            type probes;
            functionObjectLibs ("libsampling.so");
            probeLocations
            (
                (3.0  2.0  0.0)
                (3.0 -2.0  0.0)
            );
            fields
            (
                p
            );
        }
    }

	system
	{
        controlDict
        {
            endTime           0.22528;
            deltaT            3.2e-05;
            writeControl      timeStep;
            writeInterval     100;
            writeFormat       binary;
        }
	}
}

regions
{
    region0
    {
        state (transient ras);
        boundaryMesh
        {
            top
            {
                type wall;
            }
            bottom
            {
                type wall;
            }
            left
            {
                type inlet;
            }
            right
            {
                type outlet;
            }
            frontAndBack
            {
                type empty;
            }
        }
        turbulenceModel laminar;
        materials ( air );
        materialProperties
        {
            air
            {
                molWeight   28.97;
                equationOfState adiabaticPerfectFluid;
                equationOfStateCoeffs
                {
                    gamma       1.4;
                    p0          1983;
                    rho0        1.598;
                    B 0;
                }                
                thermodynamicsModel hConst;
                thermodynamicsCoeffs
                {
                    Cp          1004.5;
                    Hf          0;
                }
                muModel scalarFunction1;
                muModelCoeffs
                {
                    mu          1.458e-05;
                }
                kappaModel kappaPrandtl;
                kappaModelCoeffs
                {
                    Pr          0.7179;
                }  
                referenceFields
                {
                    p               [ 1 -1 -2 0 0 0 0 ] 101325;
                    TConst          [ 0 0 0 1 0 0 0 ] 224;
                }
            }
        }
        system
        {
               
		    fvSchemes
		    {
                ddtSchemes
                {
                    default         backward 1;
                }
                gradSchemes
                {
                    default         Gauss linear;
                }
                divSchemes
                {
                    div(phi,U)      Gauss LUST grad(U);
                }
                laplacianSchemes
                {
                    default         Gauss linear corrected;
                }
                interpolationSchemes
                {
                    default         linear;
                }
                snGradSchemes
                {
                    default         corrected;
                }
		    }
		    fvSolution
		    {
			    PIMPLE
		        {
		            nOuterCorrectors 1;
		            nCorrectors 2;
	            }
		    }
        }
        fields 
        {
            U
            {
                initialisation
                {
                    type fixedValue;
                    value uniform ( 50 0 0 );
                }
            }
            p
            {
                initialisation
                {
                    type fixedValue;
                    value uniform 0;
                }
            }
	    }
        boundaryConditions
        {
            top
            {
                U
                {
                    type slip;
                }
            }
            bottom
            {
			    U
                {
                    type slip;
                }
            }
		    
            left
            {
                
                U
                {
                    type            fixedValue;
                    value           uniform (50 0 0);
			    }
			    p
			    {
                    type            waveTransmissive;
                    field           p;
                    phi             phi;
                    rho             rho;
                    psi             psi;
                    gamma           1.4;
                    fieldInf        0;
                    lInf            5.0;
                    value           uniform 0;
			    }
            }
            right
            {
                U
                {
                    type            inletOutlet;
                    inletValue      uniform (0 0 0);
                    value           uniform (50 0 0);
                }
               	p 
			    {
                    type            waveTransmissive;
                    field           p;
                    phi             phi;
                    rho             rho;
                    psi             psi;
                    gamma           1.4;
                    fieldInf        0;
                    lInf            5.0;
                    value           uniform 0;
			    }      
            }
        }
    }
}
