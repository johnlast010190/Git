FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      loggingProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceElevation
{
    type waveGauge;

    pointDistribution lineDistribution;
    N 80;
    linestart (10 -0.3 0.05);
    lineend   (18 -0.3 0.05);
    add       ( 0 0.45 0);
    axis      y;
    stretch 1.;
}


