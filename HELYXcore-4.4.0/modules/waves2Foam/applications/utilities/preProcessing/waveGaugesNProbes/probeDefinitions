FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      loggingProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

reflectionAnalysis
{
    type waveGauge;

    pointDistribution lineDistribution;
    N 11;
    linestart (200 -3 0.05);
    lineend   (260 -3 0.05);
    add       ( 0 6 0);
    axis      y;
    stretch 1.5;
}


probing
{
    type probeGauge;

    pointDistribution lineDistribution;
    N 5;    
    linestart (35 0.1 0.0);
    lineend (39 0.9 0.0);

    outputControl timeStep;
    outputInterval 1;

    fields (U p alpha1);
}

