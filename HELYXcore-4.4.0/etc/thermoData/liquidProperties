    sigmaModelCoeffs
    {
        sigma NSRDS0 ( 0.04533 -6.88e-05 -1.6e-08 0.0 0.0 0.0 );
    }
    // NSRDS1 instead off APIdiffCoef
    Dfunc1 NSRDS1 ( 4.75e-10 1.75 0.0 0.0 0.0 );
