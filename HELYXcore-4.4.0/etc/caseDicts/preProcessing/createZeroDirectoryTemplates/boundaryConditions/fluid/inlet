FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "templates";
    object      inlet;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

subSonic
{
    nut
    {
        type        calculated;
        value       ${:VALUE.nut};
    }
    k
    {
        type        fixedValue;
        value       ${:VALUE.k};
    }
    epsilon
    {
        type        fixedValue;
        value       ${:VALUE.epsilon};
    }
    omega
    {
        type        fixedValue;
        value       ${:VALUE.omega};
    }

    OPTIONS         (flowSpecification);
}


