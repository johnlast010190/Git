FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    T   292;
}

zones
{
    burnt
    {
        type        box;
        zoneType    cell;

        box         (0.002 -0.01 0.005) (0.02 0.01 0.055);

        values
        {
            T   2200;
        }
    }
}

