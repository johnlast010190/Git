FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    T   300;
    N2  0.77;
    O2  0.23;
    CH4 0;
}

zones
{
    fuel
    {
        type        box;
        zoneType    cell;

        box (0 -10 -100) (0.0036 10 0);

        values
        {
            CH4 0.1561;
            O2  0.1966;
            N2  0.6473;
        }
    }
}

