FoamFile
{
    format      ascii;
    class       LagrangianScalarField;
    location    "0/Lagrangian/cloud";
    object      d;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [length];

internalField   uniform 2.5 [um];

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    ".*"
    {
        type            calculated;
    }
}

sources
{
    inlet
    {
        type            distributionDiameter;
        distribution
        {
            type        normal;
            Q           0;
            min         0.1 [um];
            max         5   [um];
            mu          2.5 [um];
            sigma       1   [um];
        }
    }
}


