FoamFile
{
    format          ascii;
    class           dictionary;
    object          refineMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

coordinates
{
    type            fieldBased;
    directions      (eR eTheta eZ);
}

zone
{
    type            annulus;
    point1          (0 0 -10);
    point2          (0 0 10);
    outerRadius     18.47;
    innerRadius     0;
}

