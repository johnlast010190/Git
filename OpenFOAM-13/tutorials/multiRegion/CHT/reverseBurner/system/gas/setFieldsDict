FoamFile
{
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    N2  0.79;
    O2  0.21;
    CH4 0;
}

zones
{
    fuel
    {
        type        box;
        zoneType    cell;

        box         (0 0 -0.01) (0.01 0.1 0.01);

        values
        {
            N2  0;
            O2  0;
            CH4 1;
        }
    }
}

