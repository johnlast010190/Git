FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      fDefault.gas2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            zeroGradient;
    }
    wall
    {
        type            zeroGradient;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}

sources
{
    heatTransferLimitedPhaseChange
    {
        type            interfacialGrowthSizeGroup;
    }
    heatTransferLimitedPhaseChange2
    {
        type            interfacialGrowthSizeGroup;
    }
    wallBoiling
    {
        type            NaN;
    }
}

