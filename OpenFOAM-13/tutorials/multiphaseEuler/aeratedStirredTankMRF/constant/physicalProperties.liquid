thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          eConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

mixture
{
    specie
    {
        molWeight   18.02;
    }
    equationOfState
    {
        C (0.001278 -2.1055e-06 3.9689e-09 4.3772e-13 -2.0225e-16);
        rho         997;
    }
    thermodynamics
    {
        Cp          4180;
        Cv          4135;
        hf          0;
    }
    transport
    {
        mu          8.904e-4;
        Pr          6.125;
    }
}

