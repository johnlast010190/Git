FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      rotatingZoneProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

origin  (0 0 0);
axis    (0 0 -1);

omega   500 [rpm];
nonRotatingPatches (); // List of stationary patches inside the rotatingZone

