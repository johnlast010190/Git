FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

zones
{
    gas
    {
        type        box;
        zoneType    cell;

        box         (-0.51 -0.51 0.8) (0.51 0.51 1.5);

        values
        {
            alpha.gas       1;
            alpha.liquid    0;
        }
    }
}

