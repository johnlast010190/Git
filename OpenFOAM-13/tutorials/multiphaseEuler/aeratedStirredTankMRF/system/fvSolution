FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "alpha.*"
    {
        nCorrectors     3;
        nSubCycles      3;

        MULES
        {
            nIter           1;
            globalBounds    yes;
        }

        MULESCorr       yes;
        MULESCorrRelax  0.5;

        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-10;
        relTol          0;
        minIter         1;
    }

    p_rgh
    {
        solver          GAMG;
        smoother        DIC;
        tolerance       1e-6;
        relTol          0.1;
    }

    p_rghFinal
    {
        $p_rgh;
        relTol         0;
    }

    "pcorr.*"
    {
        $p_rghFinal;
        tolerance       0.01;
        relTol          0;

    }

    "(U|k|omega).*"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-6;
        relTol          0;
    }
}

PIMPLE
{
    nOuterCorrectors    1;
    nCorrectors         3;
    nNonOrthogonalCorrectors 0;

    correctPhi          yes;

    faceMomentum        no;
    VmDdtCorrection     yes;
    dragCorrection      yes;
}

relaxationFactors
{
    equations
    {
        ".*"    1;
    }
}

