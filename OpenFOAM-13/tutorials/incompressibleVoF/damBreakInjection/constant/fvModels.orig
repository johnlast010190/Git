FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvModels;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

injection
{
    type            volumeSource;

    phase           water;

    cellZone
    {
        type            containsPoints;
        points          ((0.438 0.438 0.0073));
    }

    volumetricFlowRate 0.0003;
}


