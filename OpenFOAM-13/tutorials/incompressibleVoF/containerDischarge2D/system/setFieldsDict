FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.liquid 0;
}

zones
{
    liquid
    {
        type        box;
        zoneType    cell;

        box         (-100 -100 0.0) (100 100 0.05);

        values
        {
            alpha.liquid 1;
        }
    }
}

