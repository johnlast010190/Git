FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.water 0;
}

zones
{
    cells
    {
        type        box;
        zoneType    cell;

        box (-1e300 -1e300 -1e300) (1e300 0 1e300);

        values
        {
            alpha.water 1;
        }
    }

    patchFaces
    {
        type        box;
        zoneType    face;

        box (-1e300 -1e300 -1e300) (1e300 0 1e300);

        zone
        {
            type        patch;
            patches     (inlet outlet);
        }

        values
        {
            alpha.water 1;
        }
    }
}

// ************************************************************************* //
