FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.oil   1;
}

zones
{
    water
    {
        type        box;
        zoneType    cell;

        box         (-4 -4 -1) (4 4 0.74);

        values
        {
            alpha.oil 0;
        }
    }
}

