FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      subsetMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

zone
{
    type        box;
    select      outside;
    box         (0.375 0.375 0) (0.625 0.625 0.25);
}

patch   walls;

