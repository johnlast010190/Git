// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ignition
{
    type        cylinder;
    zoneType    cell;
    moveUpdate  false;

    point1  (1 -0.05 -0.12);
    point2  (1 0 -0.12);
    radius  0.03;
}

